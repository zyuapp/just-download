{
  "name": "@just-download/chrome-extension",
  "version": "0.1.0",
  "private": true,
  "description": "Manage Chrome downloads in the Just Download desktop app.",
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "build": "node scripts/build.cjs",
    "dev": "npm run build",
    "lint": "node -e \"process.exit(0)\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.36",
    "typescript": "^5.7.3"
  }
}
